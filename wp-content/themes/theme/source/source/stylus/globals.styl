meyer-reset()
box-sizing-reset()
normalize()
*
	outline 0
body
	font-family 'Open Sans', sans-serif
	line-height 1
img
	max-width 100%
p
	margin rem(1px 1px 10px 1px)
	&:last-child
		margin-bottom 0
figure
	margin 0
h1,
h2,
h3,
h4,
h5,
h6
	font-weight $font-bold
h1
	font-size rem(32px)
h2
	font-size rem(28px)
h3
	font-size rem(24px)
h4
	font-size rem(20px)
h5
	font-size rem(16px)
h6
	font-size rem(14px)